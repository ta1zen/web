{
  "manifest_version": 3,
  "name": "Volume Controller",
  "version": "1.0",
  "description": "Extension to control tab volume",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "iconsound.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
     "storage"
  ]
}
